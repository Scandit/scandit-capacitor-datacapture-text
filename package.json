{
  "name": "scandit-capacitor-datacapture-text",
  "version": "6.15.0",
  "description": "Scandit Data Capture SDK for Capacitor",
  "main": "dist/plugin.cjs.js",
  "module": "dist/esm/scandit-capacitor-datacapture-text/src/index.js",
  "types": "dist/esm/scandit-capacitor-datacapture-text/src/definitions.d.ts",
  "unpkg": "dist/plugin.js",
  "files": [
    "android/src/main/",
    "android/build.gradle",
    "dist/",
    "ios/Plugin/",
    "ScanditCapacitorDatacaptureText.podspec"
  ],
  "author": "Scandit",
  "license": "Apache-2.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Scandit/scandit-capacitor-datacapture-text.git"
  },
  "bugs": {
    "url": "https://github.com/Scandit/scandit-capacitor-datacapture-text/issues"
  },
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "scripts": {
